{
  "expo": {
    "name": "eKMA",
    "slug": "eKMA",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eKMA.package"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/calendar.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "minifyEnabled": true,
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo": {
      "plugins": [
        [
          "expo-local-authentication",
          {
            "faceIDPermission": "Allowed to use Face ID."
          }
        ]
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "353ba1a8-2b19-447f-b87b-8a3c8019cc81"
      }
    }
  }
}
